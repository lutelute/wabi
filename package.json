{
  "name": "wabi",
  "version": "1.3.0",
  "description": "侘び - ルーティン管理アプリ",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:web": "VITE_WEB_MODE=true vite --port 5177",
    "build": "tsc && vite build && electron-builder",
    "build:web": "VITE_WEB_MODE=true tsc --noEmit && VITE_WEB_MODE=true vite build",
    "build:mac": "tsc && vite build && electron-builder --mac",
    "install:mac": "npm run build:mac && cp -R release/*/mac-arm64/wabi.app /Applications/ && echo '✓ /Applications/wabi.app にインストール完了'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "electron-updater": "^6.6.2",
    "nanoid": "^5.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "electron-store": "^11.0.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
